blackbox emit;
import Mac_Learning;

module Startup_Emit:


type startup = {};

action forward(pkt : packet, newpkt : packet) :- Mac_Learning.forward(pkt, newpkt), not pkt.dlSrc = 0;

// No switchHasPort yet... so hardcoded.
action emit(st: startup, newpkt : packet) :- newpkt.locSw = 1, newpkt.locPt = 1;